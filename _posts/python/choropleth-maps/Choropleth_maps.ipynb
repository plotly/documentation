    "    locations=['CO', 'FL', 'MI', 'MN', 'NH', 'OH', 'VA', 'WI'],\n",
    "    name='Swing State',\n",
    "    showscale=False,\n",
    "    text=['Colorado', 'Florida', 'Michigan', 'Minnesota', \n",
    "          'New Hampshire', 'Ohio', 'Virginia', 'Wisconsin'],\n",
    "    zauto=False,\n",
    "    zmax=1,\n",
    "    zmin=0,\n",
    ")\n",
    "\n",
    "data = Data([trace1, trace2, trace3])\n",
    "layout = Layout(\n",
    "    autosize=False,\n",
    "    geo=dict(\n",
    "        countrycolor='rgb(102, 102, 102)',\n",
    "        countrywidth=0.1,\n",
    "        lakecolor='rgb(255, 255, 255)',\n",
    "        landcolor='rgba(237, 247, 138, 0.28)',\n",
    "        lonaxis=dict(\n",
    "            gridwidth=1.5999999999999999,\n",
    "            range=[-180, -50],\n",
    "            showgrid=False\n",
    "        ),\n",
    "        projection=dict(\n",
    "            type='albers usa'\n",
    "        ),\n",
    "        scope='usa',\n",
    "        showland=True,\n",
    "        showrivers=False,\n",
    "        showsubunits=True,\n",
    "        subunitcolor='rgb(102, 102, 102)',\n",
    "        subunitwidth=0.5\n",
    "    ),\n",
    "    hovermode='closest',\n",
    "    images=list([\n",
    "        dict(\n",
    "            x=1,\n",
    "            y=0.6,\n",
    "            sizex=0.155,\n",
    "            sizey=0.4,\n",
    "            source='http://i.imgur.com/Xe3f1zg.png',\n",
    "            xanchor='right',\n",
    "            xref='paper',\n",
    "            yanchor='bottom',\n",
    "            yref='paper'\n",
    "        )\n",
    "    ]),\n",
    "    showlegend=True,\n",
    "    title='<b>PACE Approved legislation</b>',\n",
    "    width= 800,\n",
    "    margin = dict(\n",
    "        l=0,\n",
