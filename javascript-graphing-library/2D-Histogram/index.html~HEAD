    <div class="section">
        <div class="row auto-eg-padding">
      <div class=" row twelve columns">
        <h3 id="2d-histogram-overlaid-with-a-scatter-chart"><a class="no_underline plot-blue" href="#2d-histogram-overlaid-with-a-scatter-chart">2D Histogram Overlaid with a Scatter Chart</a></h3>
      </div>
      <div class="row">
        <div class="six columns">
          <h4 style="margin: 40px 0 0 0;">JavaScript</h4>
          <div class="z-depth-1">
            <pre><code class="plotly_js">var x0 = [];
var y0 = [];
var x1 = [];
var y1 = [];
for (var i = 0; i &lt; 500; i ++) {
	x0[i] = Math.random() / 5 * 0.5;
	y0[i] = Math.random() / 5 * 0.5;
}

for (var i = 0; i &lt; 50; i ++) {
	x1[i] = Math.random();
	y1[i] = Math.random() + 1;
}

var x = [x0, x1]
var y = [y0, y1]

var trace1 = {
  x: x0, 
  y: y0, 
  mode: "markers", 
  marker: {
    symbol: "circle", 
    opacity: 0.7
  }, 
  type: "scatter"
};
var trace2 = {
  x: x1, 
  y: y1, 
  mode: "markers", 
  marker: {
    symbol: "square", 
    opacity: 0.7
  }, 
  type: "scatter"
};
var trace3 = {
  x: x, 
  y: y, 
  type: "histogram2d"
};
var data = [trace1, trace2, trace3];
Plotly.plot("myDiv", data);
</code></pre>
          </div>
          
          <h4 style="margin: 40px 0 0 0;">HTML</h4>
          <div class="z-depth-1">
            <pre style="background-color:rgb(239,241,245)"><code class=".html .hljs-doctype" style="background-color:rgb(239,241,245)">&lt;!DOCTYPE html&gt; 
&lt;head&gt;
  &lt;!-- D3.js --&gt;
  &lt;script src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.6/d3.min.js"&gt;&lt;/script&gt;
  &lt;!-- jQuery --&gt;  
  &lt;script src="https://code.jquery.com/jquery-2.1.4.min.js"&gt;&lt;/script&gt;
  &lt;!-- Plotly.js --&gt;   
  &lt;script src="https://d14fo0winaifog.cloudfront.net/plotly-basic.js"&gt;&lt;/script&gt;
&lt;/head&gt;
&lt;body&gt;
&lt;!-- Plotly chart will be drawn inside this DIV --&gt;
