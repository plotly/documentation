    <div class="section">
        <div class="row auto-eg-padding">
      <div class=" row twelve columns">
        <h3 id="2d-histogram-binning-and-styling-options"><a class="no_underline plot-blue" href="#2d-histogram-binning-and-styling-options">2D Histogram Binning and Styling Options</a></h3>
      </div>
      <div class="row">
        <div class="six columns">
          <h4 style="margin: 40px 0 0 0;">JavaScript</h4>
          <div class="z-depth-1">
            <pre><code class="plotly_js">var x = [];
var y = [];
for (var i = 0; i &lt; 500; i ++) {
	x[i] = Math.random();
	y[i] = Math.random() + 1;
}

var data = [
  {
    x: x, 
    y: y, 
    histnorm: "probability", 
    autobinx: false, 
    xbins: {
      start: -3, 
      end: 3, 
      size: 0.1
    }, 
    autobiny: false, 
    ybins: {
      start: -2.5, 
      end: 4, 
      size: 0.1
    }, 
    colorscale: [["0", "rgb(12,51,131)"], ["0.25", "rgb(10,136,186)"], ["0.5", "rgb(242,211,56)"], ["0.75", "rgb(242,143,56)"], ["1", "rgb(217,30,30)"]], 
    type: "histogram2d"
  }
];
Plotly.plot("myDiv", data);
</code></pre>
          </div>
          
          <h4 style="margin: 40px 0 0 0;">HTML</h4>
          <div class="z-depth-1">
            <pre style="background-color:rgb(239,241,245)"><code class=".html .hljs-doctype" style="background-color:rgb(239,241,245)">&lt;!DOCTYPE html&gt; 
